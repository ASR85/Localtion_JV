<Window x:Class="Localtion_JV.WindowAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Localtion_JV"
        mc:Ignorable="d"
        Title="WindowAdmin" Height="450" Width="800" Background="Gray">
    <Grid>
        <Frame x:Name="Main" Margin="0,49,0,0"/>
        <Button Content="Liste des jeux" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Height="34" Width="120" Click="Button_Click"/>
        <Button Content="Ajouter un jeu" HorizontalAlignment="Left" Margin="150,10,0,0" VerticalAlignment="Top" Height="34" Width="120" Click="Button_Click_1"/>
        <Button Content="Editer un jeu" HorizontalAlignment="Left" Margin="280,10,0,0" VerticalAlignment="Top" Height="34" Width="120" Click="Button_Click_2"/>
        <Button Content="Supprimer un jeu" HorizontalAlignment="Left" Margin="410,10,0,0" VerticalAlignment="Top" Height="34" Width="120" Click="Button_Click_3"/>

    </Grid>
</Window>
