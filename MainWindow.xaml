<Window x:Class="Localtion_JV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Localtion_JV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Accueil" MinWidth="100" Margin="4" Click="BtnAccueil"/>
            <Button Content="Inscription" MinWidth="100" Margin="4" Click="BtnInscription"/>
            <Button Content="Informations" MinWidth="100" Margin="4" Click="BtnDetails"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0"/>
    </Grid>
</Window>
